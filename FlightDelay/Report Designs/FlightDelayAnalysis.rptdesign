<div id="results"></div>

<script type="text/javascript">
(
  function()
  {

      lookFor = "AirportList";
      lookFor = lookFor.split("|");
      var set = 0;
      //alert(lookFor);

      var form = document.getElementById("theForm");
      var resultsDiv = document.getElementById("results");
      var searchField = form.search;
      searchField.focus();

      // first, position the results:
      var node = searchField;
      var x = 0;
      var y = 0;
      while ( node != null )
      {
          x += node.offsetLeft;
          y += node.offsetTop;
          node = node.offsetParent;
      }
      resultsDiv.style.left = x + "px";
      resultsDiv.style.top  = (y + 20) + "px";
      
      // now, attach the keyup handler to the search field:
      searchField.onkeyup = function()
      {
      	  
      	  if(set == 0){
      	  document.getElementById('searchDiv').style.height = "220px";
      	  resultsDiv.style.height = "180px";
          resultsDiv.style.overflow = "scroll";
          set = 1;
          }
          if (searchField.value.length == 0){
      			resultsDiv.style.display = "none";
                document.getElementById('searchDiv').style.height = "";
          	}
          var txt = this.value.toLowerCase();
          if ( txt.length == 0 ) return;

          var txtRE = new RegExp( "(" + txt + ")", "ig" );
          // now...do we have any matches?
          var top = 0;
          for ( var s = 0; s < lookFor.length; s++ )
          {
              var srch = lookFor[s];
              if ( srch.toLowerCase().indexOf(txt) >= 0 )
              {
                  srch = srch.replace( txtRE, "<span>$1</span>" );
                  var div = document.createElement("div");
                  div.innerHTML = srch;
                  div.onclick = function() {
                  	  searchField.blur();
                  	  resultsDiv.focus();
                      searchField.value = this.innerHTML.replace(/\<\/?span\>/ig,"");
                      resultsDiv.style.display = "none";
                      document.getElementById('searchDiv').style.height = "";
                      set = 0;
                      window.location = "http://demo.actuate.com/iportal/executereport.do?__locale=en_US&amp;__vp=Default%20Volume&amp;volume=Default%20Volume&amp;closex=true&amp;__executableName=%2FHome%2FdevSite%2FFDA2.rptdesign%3B1&amp;__requesttype=immediate&amp;__format=html&amp;Airport=" + searchField.value;
                      //http://127.0.0.1:55608/iportal/executereport.do?__locale=en_US&amp;__svg=false&amp;__timezone=US%2FIndiana-Starke&amp;__masterpage=true&amp;__dpi=96&amp;__vp=workgroup&amp;closex=true&amp;__executableName=C%3A%2FUsers%2Fmwilliams%2Fworkspace%2FiHub3%2FBMDK%2FReport%20Designs%2FFDA.rptdesign&amp;__requesttype=immediate
                      //"http://demo.actuate.com/iportal/executereport.do?__locale=en_US&amp;__vp=Default%20Volume&amp;volume=Default%20Volume&amp;closex=true&amp;__executableName=%2FHome%2FdevSite%2FFDA.rptdesign%3B1&amp;__requesttype=immediate&amp;__format=html&amp;Airport=" + searchField.value;
                  };
                  div.style.top = top + "px";
                  //div.style.borderTopColor = "black";
                  //div.style.borderTopWidth = "1px";
                  //div.style.borderTopStyle = "solid";
                  top += 20;
                  resultsDiv.appendChild(div);
                  resultsDiv.style.display = "block";
                  resultsDiv.style.borderColor = "black";
                  resultsDiv.style.borderWidth = "1px";
                  resultsDiv.style.borderStyle = "solid";
              }
          }
      }
      // and the keydown handler:
      searchField.onkeydown = function() 
      {
          while ( resultsDiv.firstChild != null )
          {
              resultsDiv.removeChild( resultsDiv.firstChild );          
          }
          resultsDiv.style.display = "none";
      }
      
      searchField.onblur = function(){
      		if (searchField.value.length == 0){
      			resultsDiv.style.display = "none";
                document.getElementById('searchDiv').style.height = "";
          	}
      }
      
  }
)();

	  
